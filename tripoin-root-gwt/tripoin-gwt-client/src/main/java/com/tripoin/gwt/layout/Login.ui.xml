<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.tripoin.gwt.resources.LoginResources"></ui:with>
    <g:HTMLPanel>
        <g:VerticalPanel addStyleNames="{res.style.background}, {res.style.pusat}">
            <g:Label text="Login" addStyleNames="{res.style.blackText}" />
            <g:TextBox ui:field="loginBox" addStyleNames="{res.style.box}"  />
            <g:Label text="Password" addStyleNames="{res.style.blackText}" />
            <g:PasswordTextBox ui:field="passwordBox" addStyleNames="{res.style.box}" />
            <g:HorizontalPanel verticalAlignment="middle">
                <g:Button ui:field="buttonSubmit" text="Submit" addStyleNames="{res.style.loginButton}" />
                <g:CheckBox ui:field="myCheckBox" />
                <g:Label ui:field="myLabel" text="Remember me" addStyleNames="{res.style.blackText}" />
            </g:HorizontalPanel>
            <g:Label ui:field="completionLabel1" addStyleNames="{res.style.blackText}" />
            <g:Label ui:field="completionLabel2" addStyleNames="{res.style.blackText}" />
        </g:VerticalPanel>
    </g:HTMLPanel>
</ui:UiBinder>